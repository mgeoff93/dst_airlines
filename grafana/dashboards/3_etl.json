{
  "title": "3 - ETL Pipeline",
  "uid": "etl_pipeline",
  "tags": ["etl", "pipeline", "airlines"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "type": "row",
      "title": "Pipeline flow"
    },
    {
      "id": 2,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 1},
      "type": "timeseries",
      "title": "Extraction",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "rate(etl_extracted_flights_total[5m]) * 60 or vector(0)", "refId": "A", "legendFormat": "Flights/min"},
        {"expr": "rate(etl_api_errors_total[5m]) * 60 or vector(0)", "refId": "B", "legendFormat": "Errors/min"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "lineWidth": 2
          },
          "min": 0
        },
        "overrides": [
          {"matcher": {"id": "byName", "options": "Flights/min"}, "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]},
          {"matcher": {"id": "byName", "options": "Errors/min"}, "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]}
        ]
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 1},
      "type": "piechart",
      "title": "Triage",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "etl_triage_total{type=\"scrape\"}", "refId": "A", "legendFormat": "Scrape"},
        {"expr": "etl_triage_total{type=\"direct\"}", "refId": "B", "legendFormat": "Direct"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "right", "values": ["percent"]},
        "pieType": "donut",
        "displayLabels": ["percent"]
      },
      "fieldConfig": {
        "defaults": {}
      }
    },
    {
      "id": 4,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
      "type": "row",
      "title": "Loading"
    },
    {
      "id": 5,
      "gridPos": {"h": 6, "w": 16, "x": 0, "y": 10},
      "type": "timeseries",
      "title": "Rows inserted",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "rate(etl_loaded_rows_total{table=\"static\"}[5m]) * 60 or vector(0)", "refId": "A", "legendFormat": "Static/min"},
        {"expr": "rate(etl_loaded_rows_total{table=\"dynamic\"}[5m]) * 60 or vector(0)", "refId": "B", "legendFormat": "Dynamic/min"},
        {"expr": "rate(etl_loaded_rows_total{table=\"live\"}[5m]) * 60 or vector(0)", "refId": "C", "legendFormat": "Live/min"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": {"mode": "normal"}
          },
          "min": 0
        }
      }
    },
    {
      "id": 6,
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 10},
      "type": "stat",
      "title": "Selenium timeouts",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "flightaware_selenium_timeouts_total or vector(0)", "refId": "A"}],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      }
    }
  ],
  "templating": {"list": []},
  "annotations": {"list": []},
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "time": {"from": "now-3h", "to": "now"},
  "timepicker": {}
}