{
  "title": "2 - Airflow Monitoring",
  "uid": "airflow_monitoring",
  "tags": ["airflow", "dags", "airlines"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "type": "row",
      "title": "Overview"
    },
    {
      "id": 2,
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
      "type": "stat",
      "title": "Scheduler",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "airflow_scheduler_heartbeat", "refId": "A"}],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{"value": null, "color": "red"}, {"value": 1, "color": "green"}]
          },
          "mappings": [
            {"type": "range", "options": {"from": 0, "to": 0, "result": {"text": "DOWN"}}},
            {"type": "range", "options": {"from": 1, "to": 999999, "result": {"text": "ACTIVE"}}}
          ]
        }
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 8, "w": 18, "x": 6, "y": 1},
      "type": "timeseries",
      "title": "Executor activity",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "airflow_executor_running_tasks", "refId": "A", "legendFormat": "Running"},
        {"expr": "airflow_executor_queued_tasks", "refId": "B", "legendFormat": "Queued"},
        {"expr": "airflow_executor_open_slots", "refId": "C", "legendFormat": "Free slots"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "lineWidth": 2
          },
          "min": 0
        }
      }
    },
    {
      "id": 4,
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 5},
      "type": "stat",
      "title": "Success rate",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "100 * (airflow_dagrun_duration_success_count / greatest(airflow_dagrun_duration_success_count + (airflow_dagrun_duration_failed_count or vector(0)), 1))", "refId": "A"}],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 95, "color": "green"}
            ]
          },
          "unit": "percent",
          "decimals": 1
        }
      }
    },
    {
      "id": 5,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
      "type": "row",
      "title": "DAG performance"
    },
    {
      "id": 6,
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 10},
      "type": "timeseries",
      "title": "DAG duration",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "airflow_dagrun_duration_success_etl_sum / airflow_dagrun_duration_success_etl_count", "refId": "A", "legendFormat": "ETL"},
        {"expr": "airflow_dagrun_duration_success_model_sum / airflow_dagrun_duration_success_model_count", "refId": "B", "legendFormat": "Model"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 0,
            "lineWidth": 2
          },
          "unit": "s",
          "min": 0
        }
      }
    },
    {
      "id": 7,
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 10},
      "type": "timeseries",
      "title": "Task execution",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "rate(airflow_dagrun_duration_success_count[5m]) * 60", "refId": "A", "legendFormat": "Success/min"},
        {"expr": "rate(airflow_dagrun_duration_failed_count[5m]) * 60 or vector(0)", "refId": "B", "legendFormat": "Failed/min"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80
          },
          "min": 0
        },
        "overrides": [
          {"matcher": {"id": "byName", "options": "Success/min"}, "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]},
          {"matcher": {"id": "byName", "options": "Failed/min"}, "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]}
        ]
      }
    },
    {
      "id": 8,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16},
      "type": "row",
      "title": "Selenium pool"
    },
    {
      "id": 9,
      "gridPos": {"h": 6, "w": 16, "x": 0, "y": 17},
      "type": "timeseries",
      "title": "Pool usage",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "airflow_pool_running_slots_selenium_pool", "refId": "A", "legendFormat": "Running"},
        {"expr": "airflow_pool_open_slots_selenium_pool", "refId": "B", "legendFormat": "Available"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 25,
            "stacking": {"mode": "normal"}
          },
          "min": 0,
          "max": 4
        }
      }
    },
    {
      "id": 10,
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 17},
      "type": "stat",
      "title": "Starving tasks",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "airflow_pool_starving_tasks_selenium_pool", "refId": "A"}],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      }
    }
  ],
  "templating": {"list": []},
  "annotations": {"list": []},
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "time": {"from": "now-3h", "to": "now"},
  "timepicker": {}
}