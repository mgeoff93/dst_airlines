{
  "title": "4 - ML Performance",
  "uid": "ml_performance",
  "tags": ["machine learning", "model", "airlines"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "type": "row",
      "title": "Model comparison"
    },
    {
      "id": 2,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 1},
      "type": "bargauge",
      "title": "R2 score",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "ml_model_r2_score{status=\"production\"} or vector(0)", "refId": "A", "legendFormat": "Production"},
        {"expr": "ml_model_r2_score{status=\"challenger\"} or vector(0)", "refId": "B", "legendFormat": "Challenger"}
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0.6, "color": "yellow"},
              {"value": 0.8, "color": "green"}
            ]
          },
          "unit": "percentunit",
          "decimals": 3,
          "noValue": "No model"
        }
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 1},
      "type": "bargauge",
      "title": "MAE (minutes)",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "ml_model_mae{status=\"production\"} or vector(0)", "refId": "A", "legendFormat": "Production"},
        {"expr": "ml_model_mae{status=\"challenger\"} or vector(0)", "refId": "B", "legendFormat": "Challenger"}
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 15, "color": "yellow"},
              {"value": 30, "color": "red"}
            ]
          },
          "unit": "m",
          "decimals": 1,
          "noValue": "No model"
        }
      }
    },
    {
      "id": 4,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
      "type": "row",
      "title": "Evolution"
    },
    {
      "id": 5,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 10},
      "type": "timeseries",
      "title": "Model metrics over time",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [
        {"expr": "ml_model_r2_score{status=\"production\"}", "refId": "A", "legendFormat": "R2 (prod)"},
        {"expr": "ml_model_mae{status=\"production\"} / 100", "refId": "B", "legendFormat": "MAE /100 (prod)"}
      ],
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15,
            "lineWidth": 2
          },
          "noValue": "No history"
        },
        "overrides": [
          {"matcher": {"id": "byRegexp", "options": "R2.*"}, "properties": [{"id": "custom.axisPlacement", "value": "left"}, {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]},
          {"matcher": {"id": "byRegexp", "options": "MAE.*"}, "properties": [{"id": "custom.axisPlacement", "value": "right"}, {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]}
        ]
      }
    },
    {
      "id": 6,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
      "type": "row",
      "title": "Operations"
    },
    {
      "id": 7,
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 19},
      "type": "stat",
      "title": "Predictions",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "sum(api_predictions_total) or vector(0)", "refId": "A"}],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "noValue": "No predictions"
        }
      }
    },
    {
      "id": 8,
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 19},
      "type": "stat",
      "title": "Latency P95",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "histogram_quantile(0.95, rate(ml_model_inference_latency_ms_bucket[5m])) or vector(0)", "refId": "A"}],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 100, "color": "yellow"},
              {"value": 200, "color": "red"}
            ]
          },
          "unit": "ms",
          "decimals": 0,
          "noValue": "No data"
        }
      }
    },
    {
      "id": 9,
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 19},
      "type": "stat",
      "title": "Training size",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "ml_training_rows_count or vector(0)", "refId": "A"}],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "noValue": "No training"
        }
      }
    }
  ],
  "templating": {"list": []},
  "annotations": {"list": []},
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "time": {"from": "now-3h", "to": "now"},
  "timepicker": {}
}